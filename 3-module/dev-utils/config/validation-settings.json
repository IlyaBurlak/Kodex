{
  "email": {
    "allowedDomains": [
      "n"
    ],
    "validateFormat": false
  },
  "password": {
    "minLength": 23,
    "requireSpecial": false,
    "requireNumber": false,
    "requireUpper": false,
    "requireLower": false,
    "forbiddenPatterns": [
      "password",
      "qwerty",
      "123456",
      "abc123"
    ],
    "maxRepeatingChars": 3,
    "maxSequentialChars": 3
  }
}